{
	"name": "driverp",
	"version": "1.4.2",
	"author": "nepaloi",
	"license": "MIT",
	"scripts": {
		"install": "concurrently \"yarn --cwd server install\" \"yarn --cwd client install\" \"yarn --cwd ui install\"",
		"build": "concurrently \"yarn build:server\" \"yarn build:client\" \"yarn build:ui\"",
		"build:server": "yarn --cwd server build",
		"build:client": "yarn --cwd client build",
		"build:ui": "yarn --cwd ui build",
		"dev:server": "yarn --cwd server dev",
		"dev:client": "yarn --cwd client dev"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"concurrently": "^5.0.2",
		"eslint": "^7.32.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.4.0",
		"typescript": "^4.4.3"
	}
}