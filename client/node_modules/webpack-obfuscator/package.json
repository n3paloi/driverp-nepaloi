{
    "name": "webpack-obfuscator",
    "version": "1.9.0",
    "description": "javascript-obfuscator plugin for Webpack",
    "keywords": [
        "obfuscator",
        "obfuscation",
        "uglify",
        "crush",
        "code protection",
        "javascript obfuscator",
        "js obfuscator",
        "webpack obfuscator"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "dependencies": {
        "acorn": "^7.3.1",
        "estraverse": "^5.1.0",
        "javascript-obfuscator": "^1.9.0",
        "loader-utils": "^2.0.0",
        "multi-stage-sourcemap": "^0.3.1",
        "multimatch": "^4.0.0",
        "webpack-sources": "^1.4.3"
    },
    "peerDependencies": {
        "webpack": "^4.44.1"
    },
    "devDependencies": {
        "@types/estraverse": "^5.1.0",
        "@types/estree": "0.0.45",
        "@types/loader-utils": "^2.0.1",
        "@types/node": "^14.0.27",
        "@types/webpack": "^4.41.21",
        "@types/webpack-sources": "^1.4.2",
        "source-map": "^0.7.3",
        "typescript": "^3.9.7",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.12"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/javascript-obfuscator/webpack-obfuscator.git"
    },
    "scripts": {
        "setup": "npm install",
        "build": "npx tsc",
        "watch": "npx tsc -w",
        "test:plugin": "npx webpack --config test/config/javascript-obfuscator-plugin.config.js",
        "test:loader": "npx webpack --config test/config/javascript-obfuscator-loader.config.js"
    },
    "author": {
        "name": "Timofey Kachalov"
